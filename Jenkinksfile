pipeline
{
    agent 
    {
        node
        {
            label 'Qa'
        }
    }
    stages
    {
        
        stage('install httpd')
        {
            steps
            {
                sh 'sudo yum install httpd -y'
                sh 'sudo chmod -R 777 /var/*'
                sh 'sudo echo "hello world" > /var/www/html/index.html'
                sh 'sudo systemctl start httpd'
            }
            
          }
    }
}



