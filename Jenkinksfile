pipeline
{
    agent 
    {
        node
        {
            label 'built-in'
        }
    }
    stages
    {
        
        stage('install httpd')
        {
            steps
            {
                sh 'sudo yum install httpd -y'
                sh 'sudo chmod -R 777 /var/*'
                sh 'sudo cp -r index.html /var/www/html'
                sh 'sudo systemctl start httpd'
            }
            
          }
    }
}



